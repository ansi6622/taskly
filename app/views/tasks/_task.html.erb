<li>
    <%= task.description %>

    <div class="actions">
        <% unless task.complete? %>
            <%= button_to [:complete, task_list, task] do %>
                Complete
            <% end %>
        <% end %>

        <%= button_to "Delete",
                      task_list_task_path(task_list, task),
                      :method => :delete, :class => "delete" %>
    </div>
</li>