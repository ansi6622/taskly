<h1>
    My Lists
    <%= link_to("Add Task List", new_task_list_path, :class => "action") %>
</h1>

<% if flash.any? %>

    <div id="flash">
        <%= flash[:notice] %>
    </div>

<% end %>

<% @task_lists.each do |task_list| %>

    <%= render task_list, :tasks => task_list.incomplete_tasks %>

<% end %>
